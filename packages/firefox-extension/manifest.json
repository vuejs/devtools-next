{
  "background": {
    "persistent": true,
    "scripts": [
      "dist/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/16-gray.png",
      "48": "icons/48-gray.png",
      "128": "icons/128-gray.png"
    },
    "default_popup": "popups/not-found.html",
    "default_title": "Vue DevTools"
  },
  "content_scripts": [
    {
      "js": [
        "dist/injection.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "dist/devtools-overlay.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle"
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "DevTools browser extension for Vue.js",
  "devtools_page": "devtools-background.html",
  "icons": {
    "16": "icons/16-beta.png",
    "48": "icons/48-beta.png",
    "128": "icons/128-beta.png"
  },
  "manifest_version": 2,
  "name": "Vue.js devtools (beta)",
  "permissions": [
    "<all_urls>"
  ],
  "version": "7.0.0.6",
  "version_name": "7.0.0 beta 6",
  "web_accessible_resources": [
    "dist/user-app.js",
    "client/devtools-panel.css",
    "client/devtools-panel.js"
  ]
}
